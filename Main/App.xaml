<Application x:Class="Timetable.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Timetable"
             Startup="Application_Startup">
    <Application.Resources>
        <Image Width="8" Height="8" x:Key="ExpandIcon" Source="Resources/ExpandIcon.ico"/>
        <Image Width="10" Height="10" x:Key="SubgroupIcon" Source="Resources/SubgroupIcon.png" x:Shared="False"/>
        <Image Width="10" Height="10" x:Key="NotesIcon" Source="Resources/NotesIcon.png" x:Shared="False"/>
    </Application.Resources>
</Application>
